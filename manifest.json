{
  "name": "Fixes",
  "version": "2",
  "manifest_version": 2,
  "description": "SomeFixes",
  "permissions": ["tabs", "http://*/*", "https://*/*", "storage"],
  "options_page": "options.html",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
   "content_scripts": [{
    "matches": ["<all_urls>","http://*/*","https://*/*"],
    "js": ["contentscript.js"],
    "run_at": "document_start"
  }],
  "background":{
      "scripts":["background.js"]
  }
}